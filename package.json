{
  "name": "web-app-erc",
  "version": "0.17.0",
  "description": "ERC: Economic RESTFul Client",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "lint": "eslint -c .eslintrc-build.json ./src > lint.txt",
    "css:sass": "gulp sass",
    "css:min": "gulp",
    "build:dev:dll": "webpack --colors --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --colors --progress --config webpack.dev.dll-jsx.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "stats": "npm run stats:json && npm run stats:report",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json --no-open --mode static"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/ZhnZhn.github.io.git"
  },
  "keywords": [
    "restful-client",
    "data-visualization",
    "chart-creation",
    "open-data",
    "opendata",
    "economic-indicators",
    "statistics-viewer",
    "macroeconomics",
    "stock-market",
    "cryptocurrency-prices"
  ],
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "big.js": "^5.2.2",
    "dompurify": "^2.0.7",
    "fetch-jsonp": "^1.1.3",
    "highcharts": "^6.2.0",
    "interactjs": "^1.3.4",
    "jsonstat": "^0.12.4",
    "leaflet": "^1.3.4",
    "localforage": "^1.5.7",
    "memoize-one": "^5.0.5",
    "query-string": "^5.1.1",
    "raven-js": "^3.27.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflux-core": "^1.0.0"
  },
  "jspm": {
    "dependencies": {
      "@babel/runtime": "npm:@babel/runtime@^7.8.3",
      "big.js": "npm:big.js@^5.2.2",
      "dompurify": "npm:dompurify@^2.0.7",
      "fetch-jsonp": "npm:fetch-jsonp@^1.1.3",
      "highcharts": "npm:highcharts@6.2.0",
      "interactjs": "npm:interactjs@1.3.4",
      "jsonstat": "npm:jsonstat@0.12.4",
      "leaflet": "npm:leaflet@1.3.4",
      "localforage": "npm:localforage@1.5.7",
      "memoize-one": "npm:memoize-one@5.0.5",
      "prop-types": "npm:prop-types@^15.7.2",
      "query-string": "npm:query-string@5.1.1",
      "raven-js": "npm:raven-js@^3.27.0",
      "react": "npm:react@^16.12.0",
      "react-dom": "npm:react-dom@^16.12.0",
      "react-test-renderer": "npm:react-test-renderer@^16.12.0",
      "reflux-core": "npm:reflux-core@^1.0.0"
    },
    "devDependencies": {}
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jspm": "^0.16.55",
    "react-test-renderer": "^16.12.0",
    "terser-webpack-plugin": "^2.3.2",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "jest": {
    "roots": [
      "<rootDir>/js/utils/",
      "<rootDir>/js/math/",
      "<rootDir>/js/adapters/",
      "<rootDir>/js/adapters/eurostat", 
      "<rootDir>/js/components/",
      "<rootDir>/js/components/zhn-sparklines/dataProcessing",
      "<rootDir>/js/components/zhn-m",
      "<rootDir>/js/flux/logic/",
      "<rootDir>/js/flux/stores/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/js/utils/",
      "<rootDir>/js/math/",
      "<rootDir>/js/adapters/",
      "<rootDir>/js/adapters/eurostat", 
      "<rootDir>/js/components/",
      "<rootDir>/js/components/zhn-sparklines/dataProcessing",
      "<rootDir>/js/components/zhn-m",
      "<rootDir>/js/flux/logic/",
      "<rootDir>/js/flux/stores/"
    ]
  }
