{
  "name": "web-app-erc",
  "version": "0.18.0",
  "description": "ERC: Economic RESTful Client",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "lint:build": "eslint -c .eslintrc-build.js ./src > lint-build.txt",
    "lint:hooks": "eslint -c .eslintrc-hooks.js ./src > lint-hooks.txt",
    "css:sass": "gulp sass",
    "css:min": "gulp",
    "build:dev:dll": "webpack --color --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --color --progress --config webpack.dev.dll-jsx.js",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "stats": "npm run stats:json && npm run stats:report",
    "stats:json": "webpack --config webpack.dll-jsx.js --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json --no-open --mode static"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/ZhnZhn.github.io.git"
  },
  "keywords": [
    "restful-client",
    "data-visualization",
    "chart-creation",
    "open-data",
    "opendata",
    "economic-indicators",
    "statistics-viewer",
    "macroeconomics",
    "stock-market",
    "cryptocurrency-prices"
  ],
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "big.js": "^6.1.1",
    "dompurify": "^2.2.8",
    "highcharts": "^9.1.2",
    "interactjs": "^1.3.4",
    "jsonstat": "^0.12.4",
    "leaflet": "^1.3.4",
    "localforage": "^1.9.0",
    "memoize-one": "^5.2.1",
    "raven-js": "^3.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflux-core": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "del": "^6.0.0",
    "eslint": "^7.30.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.0.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.2",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.46.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/js/charts",
      "<rootDir>/js/charts/plugin",
      "<rootDir>/js/utils/",
      "<rootDir>/js/math/",
      "<rootDir>/js/adapters/",
      "<rootDir>/js/adapters/eurostat",
      "<rootDir>/js/adapters/stat-json",
      "<rootDir>/js/components/hooks",
      "<rootDir>/js/components/dialogs/rows",
      "<rootDir>/js/components/watch-browser/hooks",
      "<rootDir>/js/components/zhn",
      "<rootDir>/js/components/zhn-sparklines/dataProcessing",
      "<rootDir>/js/components/zhn-m",
      "<rootDir>/js/components/zhn-menu",
      "<rootDir>/js/components/zhn-table",
      "<rootDir>/js/components/zhn-utils",
      "<rootDir>/js/flux/logic/",
      "<rootDir>/js/flux/stores/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/js/charts",
      "<rootDir>/js/charts/plugin",
      "<rootDir>/js/utils/",
      "<rootDir>/js/math/",
      "<rootDir>/js/adapters/",
      "<rootDir>/js/adapters/eurostat",
      "<rootDir>/js/adapters/stat-json",
      "<rootDir>/js/components/hooks",
      "<rootDir>/js/components/dialogs/rows",
      "<rootDir>/js/components/watch-browser/hooks",
      "<rootDir>/js/components/zhn",
      "<rootDir>/js/components/zhn-sparklines/dataProcessing",
      "<rootDir>/js/components/zhn-m",
      "<rootDir>/js/components/zhn-menu",
      "<rootDir>/js/components/zhn-table",
      "<rootDir>/js/components/zhn-utils",
      "<rootDir>/js/flux/logic/",
      "<rootDir>/js/flux/stores/"
    ]
  }
}
