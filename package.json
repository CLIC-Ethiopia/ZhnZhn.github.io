{
  "name": "web-app-erc",
  "version": "0.18.0",
  "description": "ERC: Economic RESTful Client",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "lint:build": "eslint -c .eslintrc-build.js ./src > lint-build.txt",
    "lint:hooks": "eslint -c .eslintrc-hooks.js ./src > lint-hooks.txt",
    "css:sass": "node ut/css-sass.js",
    "css:min": "node ut/css-min.js",
    "build:dev:dll": "webpack --color --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --color --progress --config webpack.dev.dll-jsx.js",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "stats": "npm run stats:json && npm run stats:report",
    "stats:json": "webpack --config webpack.dll-jsx.js --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json --no-open --mode static"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/ZhnZhn.github.io.git"
  },
  "keywords": [
    "restful-client",
    "data-visualization",
    "chart-creation",
    "open-data",
    "opendata",
    "economic-indicators",
    "statistics-viewer",
    "macroeconomics",
    "stock-market",
    "cryptocurrency-prices"
  ],
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "big.js": "^6.2.1",
    "dompurify": "^2.3.10",
    "highcharts": "^9.1.2",
    "jsonstat": "^0.12.4",
    "leaflet": "^1.3.4",
    "localforage": "^1.10.0",
    "memoize-one": "^5.2.1",
    "raven-js": "^3.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflux-core": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.4.1",
    "babel-loader": "^9.1.2",
    "clean-css": "^5.3.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.57.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/js/charts",
      "<rootDir>/js/charts/plugin",
      "<rootDir>/js/utils/",
      "<rootDir>/js/math/",
      "<rootDir>/js/adapters/",
      "<rootDir>/js/adapters/eurostat",
      "<rootDir>/js/adapters/stat-json",
      "<rootDir>/js/components/",
      "<rootDir>/js/components/hooks",
      "<rootDir>/js/components/dialogs/rows",
      "<rootDir>/js/components/dialogs/hooks",
      "<rootDir>/js/components/watch-browser/hooks",
      "<rootDir>/js/components/zhn",
      "<rootDir>/js/components/zhn-sparklines/dataProcessing",
      "<rootDir>/js/components/zhn-m",
      "<rootDir>/js/components/zhn-menu",
      "<rootDir>/js/components/zhn-tab",
      "<rootDir>/js/components/zhn-table",
      "<rootDir>/js/components/zhn-utils",
      "<rootDir>/js/flux/logic/",
      "<rootDir>/js/flux/stores/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/js/charts",
      "<rootDir>/js/charts/plugin",
      "<rootDir>/js/utils/",
      "<rootDir>/js/math/",
      "<rootDir>/js/adapters/",
      "<rootDir>/js/adapters/eurostat",
      "<rootDir>/js/adapters/stat-json",
      "<rootDir>/js/components/",
      "<rootDir>/js/components/hooks",
      "<rootDir>/js/components/dialogs/rows",
      "<rootDir>/js/components/dialogs/hooks",
      "<rootDir>/js/components/watch-browser/hooks",
      "<rootDir>/js/components/zhn",
      "<rootDir>/js/components/zhn-sparklines/dataProcessing",
      "<rootDir>/js/components/zhn-m",
      "<rootDir>/js/components/zhn-menu",
      "<rootDir>/js/components/zhn-tab",
      "<rootDir>/js/components/zhn-table",
      "<rootDir>/js/components/zhn-utils",
      "<rootDir>/js/flux/logic/",
      "<rootDir>/js/flux/stores/"
    ]
  }
}
