{
  "name": "web-app-erc",
  "version": "0.17.0",
  "description": "ERC: Economic RESTFul Client",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "bundle-sfx": "jspm bundle-sfx --minify --skip-source-maps js/index bundle.min.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "lint": "eslint -c .eslintrc-build.json ./src > lint.txt",
    "css:sass": "gulp sass",
    "css:min": "gulp",
    "build": "webpack --colors --progress",
    "build:jsx": "webpack --colors --progress --config webpack.config-jsx.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "build:all": "npm run lint && npm run css:sass && npm run css && npm run build:dll-jsx",
    "build:dev:dll": "webpack --colors --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --colors --progress --config webpack.dev.dll-jsx.js",
    "stats": "npm run stats:json && npm run stats:report",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json --no-open --mode static",
    "stats:help": "webpack-bundle-analyzer --help > out.txt"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/ZhnZhn.github.io.git"
  },
  "keywords": [
    "restful-client",
    "data-visualization",
    "chart-creation",
    "open-data",
    "opendata",
    "economic-indicators",
    "statistics-viewer",
    "macroeconomics",
    "stock-market",
    "cryptocurrency-prices"
  ],
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "big.js": "^5.2.2",
    "dompurify": "^2.0.7",
    "fetch-jsonp": "^1.1.3",
    "highcharts": "^6.2.0",
    "interactjs": "^1.3.4",
    "jsonstat": "^0.12.4",
    "leaflet": "^1.3.4",
    "localforage": "^1.5.7",
    "memoize-one": "^5.0.5",
    "query-string": "^5.1.1",
    "raven-js": "^3.27.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "reflux": "^5.0.4"
  },
  "jspm": {
    "dependencies": {
      "babel-runtime": "npm:babel-runtime@^6.26.0",
      "big.js": "npm:big.js@^5.2.2",
      "dompurify": "npm:dompurify@^2.0.7",
      "fetch-jsonp": "npm:fetch-jsonp@^1.1.3",
      "highcharts": "npm:highcharts@6.2.0",
      "interactjs": "npm:interactjs@1.3.4",
      "jsonstat": "npm:jsonstat@0.12.4",
      "leaflet": "npm:leaflet@1.3.4",
      "localforage": "npm:localforage@1.5.7",
      "memoize-one": "npm:memoize-one@5.0.5",
      "prop-types": "npm:prop-types@^15.7.2",
      "query-string": "npm:query-string@5.1.1",
      "raven-js": "npm:raven-js@^3.27.0",
      "react": "npm:react@^16.11.0",
      "react-dom": "npm:react-dom@^16.11.0",
      "react-test-renderer": "npm:react-test-renderer@^16.11.0",
      "reflux": "npm:reflux@^5.0.4",
      "systemjs": "npm:systemjs@^0.19.41"
    },
    "devDependencies": {}
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-system-import": "^1.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-processing-plugin": "^0.1.2",
    "jest": "^23.6.0",
    "jspm": "^0.16.55",
    "react-test-renderer": "^16.11.0",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/js/utils/",
      "<rootDir>/js/math/",
      "<rootDir>/js/adapters/",
      "<rootDir>/js/components/",
      "<rootDir>/js/components/zhn-sparklines/dataProcessing",
      "<rootDir>/js/components/zhn-m",
      "<rootDir>/js/flux/stores/"
    ]
  }
}
