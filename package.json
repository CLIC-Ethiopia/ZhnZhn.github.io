{
  "name": "erc",
  "version": "0.15.0",
  "description": "ERC: Economic RESTFul Client",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "bundle-sfx": "jspm bundle-sfx --minify --skip-source-maps js/index bundle.min.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "build": "webpack --colors --progress",
    "build:jsx": "webpack --colors --progress --config webpack.config-jsx.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.config-dll-jsx.js",
    "stats": "npm run stats:json && npm run stats:report",
    "stats:json": "webpack --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json --no-open --mode static",
    "stats:help": "webpack-bundle-analyzer --help > out.txt",
    "gulp": "gulp",
    "lint": "eslint src",
    "start": "http-server -c-1",
    "start:emitter": "chokidar-socket-emitter"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/ZhnZhn.github.io.git"
  },
  "keywords": [
    "restful-client",
    "data-visualization",
    "chart-creation",
    "open-data",
    "opendata",
    "economic-indicators",
    "statistics-viewer",
    "stock-market",
    "cryptocurrency-prices",
    "macroeconomics"
  ],
  "dependencies": {
    "accounting": "^0.4.1",
    "babel-runtime": "^6.23.0",
    "big.js": "^3.1.3",
    "dompurify": "^0.9.0",
    "fetch-jsonp": "^1.1.3",
    "highcharts": "^5.0.14",
    "interact.js": "^1.2.8",
    "jsonstat": "^0.12.4",
    "localforage": "^1.5.0",
    "lodash.flow": "^3.5.0",
    "lodash.merge": "^4.6.0",
    "query-string": "^4.3.2",
    "raven-js": "^3.14.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reflux": "^5.0.4"
  },
  "jspm": {
    "dependencies": {
      "accounting": "npm:accounting@^0.4.1",
      "babel-runtime": "npm:babel-runtime@^6.23.0",
      "big.js": "npm:big.js@^3.1.3",
      "dompurify": "npm:dompurify@^0.9.0",
      "fetch-jsonp": "npm:fetch-jsonp@^1.1.1",
      "highcharts": "npm:highcharts@^5.0.14",
      "interact.js": "npm:interact.js@^1.2.8",
      "jsonstat": "npm:jsonstat@^0.12.2",
      "leaflet": "npm:leaflet@^1.0.1",
      "localforage": "npm:localforage@^1.5.0",
      "lodash.flow": "npm:lodash.flow@^3.5.0",
      "lodash.get": "npm:lodash.get@^4.4.2",
      "lodash.merge": "npm:lodash.merge@^4.6.0",
      "prop-types": "npm:prop-types@^15.6.0",
      "query-string": "npm:query-string@^4.3.2",
      "raven-js": "github:getsentry/raven-js@^3.14.2",
      "react": "npm:react@16.2.0",
      "react-dom": "npm:react-dom@16.2.0",
      "reflux": "npm:reflux@^5.0.4",
      "systemjs": "npm:systemjs@^0.19.41"
    },
    "devDependencies": {
      "systemjs-hot-reloader": "npm:systemjs-hot-reloader@^1.1.0"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-system-import": "^1.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chokidar-socket-emitter": "^0.6.0",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.4",
    "gulp-concat": "^2.6.0",
    "gulp-inject": "^4.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-processing-plugin": "^0.1.2",
    "jest": "^19.0.2",
    "jspm": "^0.16.53",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.2.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/js/utils/",
      "<rootDir>/js/math/",
      "<rootDir>/js/adapters/",
      "<rootDir>/js/components/zhn-sparklines/dataProcessing"
    ]
  }
}
